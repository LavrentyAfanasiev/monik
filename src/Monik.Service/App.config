<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="Url" value="http://localhost:2211" />
    <!--текущее состояние программы(dev/prod/...):-->
    <add key="InstanceName" value="Dev" />
    <!--Main connection string and provider (PostgreSql or SqlServer)-->
    <add key="DBConnectionString" value="[...]" />
    <add key="DBProvider" value="SqlServer"/>
    <!--Cleanup settigns-->
    <add key="DayDeepLog" value="14" />
    <add key="DayDeepKeepAlive" value="1" />
    <add key="CleanupBatchSize" value="1000" />
    <!--Write Bulk Options-->
    <add key="WriteBatchSize" value="1000" />
    <add key="WriteBatchTimeout" value="60" />
    <!--Authentication-->
    <add key="AuthSecretKey" value="your-256-bit-secret" />
  </appSettings>
</configuration>